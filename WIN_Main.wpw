   -
     name : OPT_Ok
     identifier : 0x287634e708d39bd3
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1-
          
          HReset(FORM_MOVEMENT)
          
          IF garrCurrentEntryExit..Occurrence <> 0 THEN
          	// Create the movement form
          	FORM_MOVEMENT.MovementDate = Today()
          	FORM_MOVEMENT.MovementTime = TimeSys()
          	FORM_MOVEMENT.MovementType = gnTypeCurrentMovement
          	
          	IF HAdd(FORM_MOVEMENT) THEN
          		FOR EACH stEs of garrCurrentEntryExit
          			HReset(ENTRIES_EXITS)
          			
          			// Create the entry/exit
          			ENTRIES_EXITS.BarCode = stEs.BarCode
          			ENTRIES_EXITS.DateEE = Today()
          			ENTRIES_EXITS.FORM_MOVEMENTID = FORM_MOVEMENT.FORM_MOVEMENTID
          			ENTRIES_EXITS.QtyEE = stEs.Quantity
          			IF HAdd(ENTRIES_EXITS) THEN
          				// Modify the stock
          				HReadSeek(PRODUCT,BarCode, stEs.BarCode)
          				IF HFound(PRODUCT) THEN
          					IF gnTypeCurrentMovement = csInMode THEN
          						PRODUCT.QtyStocks += stEs.Quantity
          					ELSE
          						PRODUCT.QtyStocks -= stEs.Quantity
          					END
          					HModify(PRODUCT)
          				END
          			ELSE
          				Error("Error while creating the movement form.",HErrorInfo())
          				RETURN
          			END
          		END
          		
          		LooperDisplay(LOOP_PRODUCT,taCurrentSelection)
          		ChangePlane(1)
          		
          		ToastDisplay("Movement successfully added." + CR + "The stock was recalculated.",toastLong)
          	ELSE
          		Error("Error while creating the movement form.",HErrorInfo())
          	END
          ELSE
          	Error("Enter at least one product for this movement.")
          END
         type : 32
