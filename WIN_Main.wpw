     code : |1-
      
      // Summary: Refreshes the looper for entries/exits
      //
      PROCEDURE RefreshLPREntriesExits()
      
      LooperDeleteAll(LOOP_Movement_list)
      
      FOR EACH stES of garrCurrentEntryExit
      	LooperAddLine(LOOP_Movement_list,stES.BarCode,stES.ProductCaption,stES.Quantity)
      END
