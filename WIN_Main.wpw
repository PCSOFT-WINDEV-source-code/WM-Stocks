  procedures :
   -
     name : ChangePlane
     procedure_id : 1762654470908517331
     type_code : 14
     code : |1-
      // Summary: Change the plane
      // Syntax:
      //ChangePlane (<nPlane> is int)
      //
      // Parameters:
      //	nPlane (integer): Requested plane
      // 
      PROCEDURE ChangePlane(nPlane is int)
      
      // Options
      OPT_EntryForm..Visible = False
      OPT_Exit_Form..Visible = False
      OPT_Ok..Visible = False
      OPT_Cancel..Visible = False
      
      SWITCH nPlane
      	CASE 1
      		MyWindow..Title = "Inventory"
      		
      		// Options
      		OPT_EntryForm..Visible = True
      		OPT_Exit_Form..Visible = True
      		
      		// Change of plane
      		MyWindow..Plane = nPlane
      	CASE 2
      		MyWindow..Title = "Entry form"
      		gnTypeCurrentMovement = csInMode
      		ArrayDeleteAll(garrCurrentEntryExit)
      		EDT_BarCode = ""
      		EDT_Quantity = 1
      		RefreshLPREntriesExits()
      		
      		// Options
      		OPT_Ok..Visible = True
      		OPT_Cancel..Visible = True
      		
      		// Change of plane
      		MyWindow..Plane = 2
      	CASE 3
      		MyWindow..Title = "Exit form"
      		gnTypeCurrentMovement = csOutMode
      		ArrayDeleteAll(garrCurrentEntryExit)
      		EDT_BarCode = ""
      		EDT_Quantity = 1
      		RefreshLPREntriesExits()
      		
      		// Options
      		OPT_Ok..Visible = True
      		OPT_Cancel..Visible = True
      		
      		// Change of plane
      		MyWindow..Plane = 2
      	OTHER CASE
      		
      END
      
      
      SIPVisible(False)
