    name : BTN_ScanBarCode
    identifier : 0x287634e708ca9bd3
    internal_properties : 
    properties :
     x : 447
     y : 49
     width : 72
     height : 47
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         cbCode is a BarCodes
         sBarCode is string
         
         // Capture the bar code
         cbCode = BCCapture()
         sBarCode = cbCode..RoughValue
         
         // Run the test of the bar code
         IF ErrorOccurred = False _AND_ sBarCode <> "" THEN
         	// Is it a valid bar code?
         	HReadSeek(PRODUCT,BarCode,sBarCode)
         	IF HFound(PRODUCT) THEN
         		EDT_BarCode = sBarCode
         		BTN_Reinitialize..State = Active
         		gsCaptionCurrentProduct = PRODUCT.ProductCaption
         		
         
         <SI CibleExécution=Android>
         	
         			ReturnToCapture(EDT_Quantity)
         	
         <SINON SI CibleExécution=iOS>
         	
         	RETURN
         	
         <FIN>
         
         		
         		
         	ELSE
         		Error("Product unknown in the database")
         	END
         END
        type : 18
      -
        type : 161
        enabled : false
  -
    name : BTN_Add_ES
    identifier : 0x287634e708cb9bd3
    internal_properties : 
    properties :
     x : 94
     y : 200
     width : 156
     height : 48
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         // Bar code
         IF EDT_BarCode ~= "" THEN
         	Error("Enter a valid bar code.")
         	RETURN
         END
         HReadSeek(PRODUCT,BarCode,EDT_BarCode)
         IF NOT HFound(PRODUCT) THEN
         	Error("No product corresponds to this bar code.")
         	RETURN
         END
         
         nQuantity is int = EDT_Quantity
         
         if gnTypeCurrentMovement = csOutMode _and_ nQuantity > PRODUCT.QtyStocks then
         	error(StringBuild("	You only have %1 products in stock",PRODUCT.QtyStocks))
         	RETURN
         FIN
         
         
         // Quantity
         IF nQuantity < 1 THEN
         	Error("Enter a positive quantity.")
         
         <SI CibleExécution=Android>
         	
         		ReturnToCapture(EDT_Quantity)
         	
         <SINON SI CibleExécution=iOS>
         	
         	RETURN
         	
         <FIN>
         
         	
         END
         
         // Variable
         stES is STEntryExit
         
         // Fill the structure
         stES.BarCode = EDT_BarCode
         stES.ProductCaption = gsCaptionCurrentProduct
         stES.Quantity = EDT_Quantity
         
         // Add into the table
         ArrayAdd(garrCurrentEntryExit,stES)
         EDT_BarCode = ""
         EDT_Quantity = 1
         
         RefreshLPREntriesExits()
